ynth_xml=&quot;$work_dir/SynthesizedDistribution.xml&quot;&#10;dist_xml=&quot;$work_dir/Distribution.xml&quot;&#10;productbuild --synthesize --package &quot;$bundle_pkg&quot; &quot;$synth_xml&quot;&#10;xsltproc -o &quot;$dist_xml&quot; &quot;$PROJECT_DIR/Installer Resources/customize_pkg.xsl&quot; &quot;$synth_xml&quot;&#10;dist_pkg_name=&quot;$PRODUCT_NAME-$version.pkg&quot;&#10;productbuild --distribution &quot;$dist_xml&quot; --package-path &quot;$work_dir&quot; &quot;$work_dir/$dist_pkg_name&quot;&#10;&#10;echo &quot;# Signing installer with $CODESIGN_PKG_ID&quot;&#10;productsign --sign &quot;$CODESIGN_PKG_ID&quot; &quot;$work_dir/$dist_pkg_name&quot; &quot;$dist_dir/$dist_pkg_name&quot;&#10;&#10;open -R &quot;$dist_dir/$dist_pkg_name&quot;&#10;"
